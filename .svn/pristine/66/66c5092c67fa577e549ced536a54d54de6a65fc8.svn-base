.marquee { display: inline-block; position: relative; overflow: hidden; width: 100%; height: 40px; background-color: #fff; }
.marquee:before, .marquee:after { position: absolute; top: 0; width: 10rem; height: 100%; content: ""; z-index: 1; }
.marquee > ul { display: flex; position: absolute; height:100%; margin: 0; padding: 0; animation: marqee 30s linear infinite; animation-ms-: marqee2 30s linear infinite;}
.marquee > ul > li { margin-right: 2em; padding-top:6px; color: #222; font-size: 14px; white-space: nowrap; }
@keyframes marqee {
  0% { left: 100%; transform: translateX(0%); }
  100% { left: 0%; transform: translateX(-100%); }
}
@keyframes marqee2 {
  0% { left: 100%; transform: translateX(0%); }
  100% { left: -70%; transform: translateX(-100%); }
}

