@charset "utf-8";
/* main css */

/* layout */
body { color: #7b7b7b; font-size: 13px; line-height:18px; font-weight: normal; font-family: 'Noto Sans KR', sans-serif; letter-spacing: 0; background: #f1f1f5; }

.con_left { float: left; }
.con_right { float: right; }
.con_center { position: relative; width: 1150px; margin: 0 auto; }

.con_wrap { min-height: 660px; padding: 20px 0; background: #f1f1f5; }
.half_wrap { position: relative; width: 49%; }
.con40_wrap { position: relative; width: 39%; }
.con60_wrap { position: relative; width: 59%; }
.left_wrap { float: left; width: 188px; background: #fff; border: 1px solid #e2e2ec; }
.left_top { height: 50px; padding: 20px 0; color: #fff; font-size: 17px; font-weight: 700; text-align: center; background: #178de6; } 
.left_top p { margin-top: 10px; }

.left_wrap ul li { padding: 20px 0; padding-left: 20px; color: #5a5a5a; font-size: 15px; font-weight: bold; border-bottom: 1px solid #e5e5e5; }
.left_wrap ul li.on { color: #178de6; font-weight: 600; background: url(../images/icon_arrow.gif) no-repeat 88% 50%; }
.left_wrap ul li a { display: block; }
.left_wrap ul li a:hover { width: 100%; color: #353535; background: url(../images/icon_arrow02.gif) no-repeat 88% 46%; }

.contents_wrap { float: right; width: 880px; min-height: 650px; padding: 30px; background: #fff; border: 1px solid #e2e2ec; border-radius: 10px; box-shadow: 3px 3px #e5e9ee; }
.contents_wrap h2 { margin-bottom: 20px; padding-bottom: 20px; color: #191919; font-size: 20px; font-weight: 500; border-bottom: 1px solid #e7e7e7; }
.contents_wrap_01 { position: relative; width: 460px; margin: 100px auto; padding: 30px 20px; background: #fff; border: 1px solid #e2e2ec; border-radius: 10px; box-shadow: 3px 3px #e5e9ee; }
.contents_wrap_02,
.contents_wrap_03,
.contents_wrap_04,
.contents_wrap_05 { float: right; position: relative; width: 880px; margin-bottom: 15px; padding: 20px 30px; background: #fff; border: 1px solid #e2e2ec; border-radius: 10px; box-shadow: 3px 3px #e5e9ee; }
.contents_wrap_03 { min-height: 586px;  }
.contents_wrap_04 { min-height: 548px; }
.contents_wrap_05 { min-height: 680px; }

.scroll { overflow-y: scroll; height: 132px; }
.scroll_x { overflow-x: scroll; }
.w40p + .scroll_x { width: px; }
.w65p + .scroll_x { width: 616px; }
.scroll_x th, .scroll_x td { padding: 0 4px; }
.half_scroll { overflow-y: scroll;height: 477px; }
.scroll input.data { width: 42px; }
.scroll input.cdName { width: 110px; }
.scroll input.groupCd { width: 68px; }
.scroll input.commonCd { width: 68px; }
.scroll tbody#searchTable1 th, .scroll tbody#searchTable1 td { padding: 0; text-align: center; }
.scroll tbody#searchTable2 th, .scroll tbody#searchTable2 td { padding: 0; text-align: center; }

/* pop-up */
.overlay { opacity: 0; visibility: hidden; position: fixed; z-index: 9999; top: 0; bottom: 0; left: 0; right: 0; background: rgba(0, 0, 0, 0.5); transition: opacity 0; }
.overlay:target { opacity: 1; visibility: visible; }

.loading_wrap { position: relative; width: 400px; height: 380px; padding: 20px 0 0; background: #2a3557; }
.loading_wrap p { margin-top: 100px; color: #fff; font-size: 20px; line-height: 28px; text-align: center; }
.loading_wrap .loading { width: 20px; height: 20px; margin: 30px auto 0; padding: 6px; border: 4px dashed #fff; border-radius: 100%; }
/* .loading_wrap .loading { width: 20px; height: 20px; margin: 10px auto 0; padding: 6px; border: 4px dashed #fff; border-radius: 100%; animation: loading 2s 0s cubic-bezier(0.17, 0.37, 0.43, 0.67) infinite; } */
 .loading_wrap .excelLoading { position: absolute; top: 245px; left: 192px; }
@keyframes loading { 0 { transform: rotate(0deg); } 50% { transform: rotate(180deg); } 100% { transform: rotate(360deg); } }

.popup { position: relative; padding: 20px; text-align: center; background: #fff; border: 1px solid #178de6; box-shadow: 2px 2px 2px #ccc; transition: all 5s ease-in-out; }
.popup p { display: block; }
.popup img{ width: 600px; max-height: 500px; }
.popup_file {width: 490px;}
.popup input[type="checkbox"]{ margin-top: 2px; }
.popup .close { position: absolute; top: -20px; right: -20px; margin: 0; padding: 0; cursor: pointer; }
.popup .close img { width: 100%; }

.pop_s01 { width: 650px; margin: 80px auto; }
.pop_s02 { width: 400px; margin: 140px auto; overflow-y: auto; }
.pop_s03 { width: 400px; margin: 80px auto; height: 400px; }
.pop_s04 { width: 400px; margin: 80px auto; height: 470px; }

.pop_s02 .contents_wrap_02 { width: 340px; margin-bottom: 0px; }
.pop_s03 .contents_wrap_02 { width: 340px; }
.pop_s04 .contents_wrap_02 { width: 340px; }

.pop_s02 .contents_wrap_02 .popup_text table { margin: 5px auto; }
.pop_s02 .contents_wrap_02 .popup_text select { width: 260px; }
.pop_s02 .contents_wrap_02 .popup_text input[type=text] { width: 260px; }
.pop_s02 .contents_wrap_02 .popup_text input.calendar { width: 118px; }
.pop_s03 .contents_wrap_02 .popup_search { height: 30px; margin-bottom: 10px; padding: 20px 20px; background: #fff; border: 1px solid #e2e2ec; border-radius: 10px; box-shadow: 3px 3px #e5e9ee; }
.pop_s03 .contents_wrap_02 .popup_search input { width: 160px; }
.pop_s03 .contents_wrap_02 .popup_search input.calendar { width: 64px; background: #fff; }
.pop_s03 .contents_wrap_02 .popup_search_result { height: 200px; background: #fff; border: 1px solid #e2e2ec; border-radius: 10px; box-shadow: 3px 3px #e5e9ee; }
.pop_s03 .contents_wrap_02 .popup_search_result2 { height: 283px; background: #fff; border: 1px solid #e2e2ec; border-radius: 10px; box-shadow: 3px 3px #e5e9ee; }
.pop_s03 .contents_wrap_02 .popup_search_result .scroll { height: 167px; overflow-y: scroll; }
.pop_s03 .contents_wrap_02 .popup_search_result2 .scroll2 { height: 250px; overflow-y: scroll; }
.pop_s04 .contents_wrap_02 .popup_text { margin: 30px auto; }
.pop_s04 .contents_wrap_02 .popup_text h3 { margin: 0px auto 10px; }
.pop_s04 .contents_wrap_02 .popup_text h3.ma10 { margin: 18px auto 10px; }
.pop_s04 .contents_wrap_02 .popup_text input.popup_file { float: left; width: 280px; }
.pop_s04 .contents_wrap_02 .popup_text .btn_file { float: right; margin-right: 0px; }
.pop_s04 .contents_wrap_02 .popup_text textarea { width: 100%; margin: 0; padding: 0 4px; border: 1px solid #dbe2e7; resize: none; }

/* imaged preview - 이미지가 표시될 지역*/
.filebox .upload-display { margin-bottom: 5px; }
@media(min-width: 768px) {
	.filebox .upload-display { display: inline-block; margin-right: 5px; margin-bottom: 0; }
}
/* imaged preview - 추가될 이미지를 감싸는 요소 */ 
.filebox .upload-thumb-wrap { display: inline-block; width: 54px; padding: 2px; vertical-align: middle; background-color: #fff; border: 1px solid #ddd; border-radius: 5px; }
/* imaged preview - 추가될 이미지 */
.filebox .upload-display img { display: block; max-width: 100%; width: 100%\9; height: auto; }

/* navigation */
.logo img { position: absolute; top: -5px; left: 12px; }
.navi { display: block; clear: both; height: 100px; color: #fff; background: #2a3557; }
.navi { background: -webkit-linear-gradient(top, #353866, #2a3557); background: -o-linear-gradient(top, #353866, #2a3557); background: -moz-linear-gradient(top, #353866, #2a3557); background: linear-gradient(top, #353866, #2a3557); }
.navi ul { position: absolute; top: -5px; right: 10px; }
.navi ul li { display: inline-block; margin-top: 30px; margin-left: 38px; font-size: 16px; }
.navi ul li.on { color: #ffae00; font-weight: bold; }
.navi ul li a { color: #fff; font-weight: bold; }
.navi ul li a:hover { color: #ffae00; font-weight: bold; }

.top_quick { padding-top: 15px; font-size: 12px; text-align: right; }
.top_quick p { display: inline; vertical-align: middle; }
.top_quick span.name { color: #02cbf7; font-weight: bold; }
.top_quick span.time { color: #f9d423; padding-right: 8px; }
.top_quick img.clock { margin-left: 12px; margin-right: 6px; }
.con_center img.pdf { margin-left: 4px; }
.top_quick span.monitor { color: #fff; font-weight: bold; }
.top_quick .btn_top { margin-left: 4px; line-height: 20px; }
.top_quick .partition::after { content: " | "; color: #fff; margin : 0px 14px; }

/* footer */
.footer_wrap { clear: both; padding: 12px 0; color: #585858; line-height: 22px; background: #fff; border-top: 1px solid #ebecf0; }
.footer_wrap ul { }
.footer_wrap ul li { display: inline-block; }
.footer_wrap ul li.blank { padding: 0 12px; color: #acacac; }
.footer_wrap p { float: left; display: block; margin-bottom: 20px; font-size: 13px; font-weight: normal; }
.footer_wrap p span { color: #6a1414; }

/* tab */
ul.c_tab { font-size: 0; margin-bottom: 15px; border-bottom: 1px solid #dadada; width: 100%; }
ul.c_tab li { height: 20px; width: 100px; padding: 7px 0; display: inline-block; vertical-align: middle; font-size: 13px; text-align: center; border: 1px solid #dadada; border-bottom: none; cursor: pointer; }
ul.c_tab li.active { background: #adadad; color: #fff; font-weight: 700; border: 1px solid #939393; border-bottom: none; }

.tab_container .tab_content ul { width: 100%; margin: 0px; padding: 0px; } 
.tab_container .tab_content ul li { padding: 5px; list-style: none }

