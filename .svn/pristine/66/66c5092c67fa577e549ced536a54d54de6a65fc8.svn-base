/* popup */
.slide_area { position: relative; width: 1150px; height: 60vh; margin: 20vh auto; }
.slide_area .btn_close { position: absolute; z-index: 99; top: -30px; right: -40px; cursor: pointer; }

.slide_wrap, .container { position: relative; width: 100%; }
.container { display: table; position: relative; margin: 0; padding: 0; text-align: center; overflow: hidden; }
.container .image-box, .container .text-box { display: table-cell; margin: 0 auto; vertical-align: middle; overflow: hidden; background: #fdfdfd; border: 1px solid #dbe2e7; }
.container .image-box { border-right: none; border-radius: 10px 0 0 10px; }
.container .image-box img.carImage { width: 700px; margin: 0 auto; border-radius: 10px; }
.container .text-box { border-left: none; border-radius: 0 10px 10px 0; }
.container .text-box ul { width: 400px; margin: 0px auto; }
.container .text-box ul li { vertical-align: middle; }
.container .text-box ul li.title { padding: 16px 10px 6px; font-size: 1em; font-weight: bold; line-height: 1.8em; text-align: left; }
.container .text-box ul li.button_wrapper { padding-bottom: 30px; }
.container .text-box ul li input { width: 380px; margin: 0 auto; padding: 4px 0; font-size: 1.4em; text-align: center; letter-spacing: 0.06em; }
.container .text-box ul li table { width: 380px; margin: 0 auto; border-top: 2px solid #d1d1e1; }
.container .text-box ul li table th { padding: 0 4px; color: #7b7b7b; text-align: center; font-weight: semi-blod; background: #f4f4f9; border: 1px solid #dbe2e7; border-left: 0; }
.container .text-box ul li table td { padding: 0 4px; color: #7b7b7b; text-align: center; background: #ffffff; border: 1px solid #dbe2e7; border-left: 0; }

.arrow { position: relative; font-size: 2em; line-height: 1.4em; color: #fff; background: #222; border-radius: 50%; cursor: pointer; }
.left-arrow 	{ margin-right: 10px; }
.right-arrow 	{ margin-left: 10px; }



